generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid())
  email      String    @unique
  password   String
  name       String
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt

  @@map("users")
}
