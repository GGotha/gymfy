{
  "name": "@gymfy/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --exit-child --transpile-only src/app.ts --ignore-watch ./node_modules",
    "start:development": "ts-node -r tsconfig-paths/register src/app.ts --ignore-watch ./node_modules",
    "start:production": "export NODE_ENV=production && node dist/app.js --ignore-watch ./node_modules",
    "db:sync": "prisma migrate dev",
    "db:seed": "ts-node ./prisma/seed.ts",
    "db:truncate": "ts-node ./prisma/truncate.ts",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.5",
    "@graphql-codegen/typescript-resolvers": "2.5.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "prisma": "^3.13.0",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.3",
    "@graphql-tools/schema": "^8.3.2",
    "@graphql-tools/utils": "^8.6.2",
    "@prisma/client": "^3.13.0",
    "apollo-server": "^3.10.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
